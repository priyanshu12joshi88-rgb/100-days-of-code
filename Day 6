qs 1
#include <stdio.h>

int main() {
    int num;

    scanf("%d", &num);

    
    if (num % 2 == 0) {
        printf("%d is even\n", num);
    } else {
        printf("%d is odd\n", num);
    }

    return 0;
}
qs2
#include <stdio.h>

int main() {
    int num;

    
    scanf("%d", &num);

    
    if (num >= 0) {
        if (num == 0) {
            printf("Zero\n");
        } else {
            printf("Positive\n");
        }
    } else {
        printf("Negative\n");
    }

    return 0;
}
