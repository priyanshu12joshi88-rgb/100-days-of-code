qs1
  #include <stdio.h>

int main() {
    char ch;

    scanf("%c", &ch);

    
    if (ch >= 'A' && ch <= 'Z') {
        printf("Uppercase alphabet\n");
    } 
    else if (ch >= 'a' && ch <= 'z') {
        printf("Lowercase alphabet\n");
    } 
    else if (ch >= '0' && ch <= '9') {
        printf("Digit\n");
    } 
    else {
        printf("Special character\n");
    }

    return 0;
}
qs2
  #include <stdio.h>

int main() {
    int a, b, c;

  
    scanf("%d %d %d", &a, &b, &c);

    
    if (a >= b && a >= c) {
        printf("Largest is %d\n", a);
    } 
    else if (b >= a && b >= c) {
        printf("Largest is %d\n", b);
    } 
    else {
        printf("Largest is %d\n", c);
    }

    return 0;
}
