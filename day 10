qs1
  #include <stdio.h>

int main() {
    int a, b, c;

   
    scanf("%d %d %d", &a, &b, &c);

   
    if (a + b > c && a + c > b && b + c > a) {
        if (a == b && b == c) {
            printf("Equilateral\n");
        } 
        else if (a == b || b == c || a == c) {
            printf("Isosceles\n");
        } 
        else {
            printf("Scalene\n");
        }
    } 
    else {
        printf("Not a valid triangle\n");
    }

    return 0;
}
qs2
  #include <stdio.h>

int main() {
    int day;


    scanf("%d", &day);

 
    switch(day) {
        case 1:
            printf("Monday\n");
            break;
        case 2:
            printf("Tuesday\n");
            break;
        case 3:
            printf("Wednesday\n");
            break;
        case 4:
            printf("Thursday\n");
            break;
        case 5:
            printf("Friday\n");
            break;
        case 6:
            printf("Saturday\n");
            break;
        case 7:
            printf("Sunday\n");
            break;
        default:
            printf("Invalid day\n");
    }

    return 0;
}
